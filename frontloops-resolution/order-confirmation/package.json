  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.sass --watch css/style.css ",
    "server": "live-server",
    "start": "npm-run-all --parallel server watch:sass",
    "compile:sass": "sass css/style.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "cleancss -o css/style.min.css css/style.prefix.css",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
